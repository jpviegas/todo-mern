{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "prettier",
        "airbnb-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./client/tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0,
      "no-console": 2
    }
}
